#!/bin/sh
cat <<EOS
<!-- do not edit: generated by make-index -->
<head>
<title>Agama D-Bus API</title>
<link rel="stylesheet" href="refentry-agama.css" type="text/css">
</head>
EOS

echo "<ul>"
for F in tmp/ref-*.xml; do
  F=${F#tmp/}
  F=${F%.xml}
  echo "<li><a href=\"${F}.html\""
  echo "                >${F#ref-}</a></li>"
done
echo "</ul>"
echo "<a href=\"..\">up</a>"
